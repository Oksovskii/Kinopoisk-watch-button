{
    "manifest_version": 3,
    "name": "KP redirect extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "main.html",
      "default_icon": "icons/kp.png"
    },

    "content_scripts": [
    {
      "matches": ["https://www.kinopoisk.ru/*"],
      "js": ["scripts/main.js"]
    }
    ]
  }